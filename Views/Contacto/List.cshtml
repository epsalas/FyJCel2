@model IEnumerable<FyJCel.Models.Contacto>


<h2>Lista de Clientes</h2>



<table class="table table-hover">
    <thead class="thead-dark">
        <tr>
                
                <th scope="col">
                    
                </th>
                <th scope="col">
                    Nombres
                </th>
                <th scope="col">
                    Apellidos
                </th>
                <th scope="col">
